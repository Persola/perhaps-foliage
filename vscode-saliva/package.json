{
  "name": "saliva-repl-vscode-saliva",
  "displayName": "Saliva REPL VSCode (Saliva)",
  "version": "0.0.2",
  "description": "Saliva language integration for VSCode extension build of Saliva REPL",
  "author": "Luke Persola",
  "publisher": "persola",
  "main": "./built/extension.js",
  "engines": {
    "vscode": "^1.58.0"
  },
  "activationEvents": [
    "onCustomEditor:saliva-repl.saliva-repl-custom-editor"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "saliva-repl.saliva-repl-custom-editor",
        "displayName": "Perhaps Foliage",
        "selector": [
          {
            "filenamePattern": "*.foli.json"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "clean": "mkdir -p ./built; rm -dr ./built/*",
    "build": "npm run clean; webpack --config ./webpack.config.js",
    "start": "code --extensionDevelopmentPath=$(pwd)",
    "type": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx ."
  },
  "dependencies": {
    "saliva-repl-vscode": "file:../vscode",
    "saliva": "file:../saliva"
  },
  "devDependencies": {
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "css-loader": "~5.2.6",
    "eslint": "~7.30.0",
    "eslint-config-airbnb": "~18.2.1",
    "json-loader": "~0.5.7",
    "source-map-loader": "^3.0.0",
    "style-loader": "~3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "~5.44.0",
    "webpack-cli": "~4.7.2",
    "yaml-loader": "~0.6.0"
  }
}
