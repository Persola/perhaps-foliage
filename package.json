{
  "name": "saliva-repl",
  "version": "0.0.1",
  "description": " sdlfwhoflfawelfea",
  "main": "serve.js",
  "scripts": {
    "clean": "rm -dr serve/*",
    "build": "webpack",
    "webpack-dev-server": "webpack-dev-server server.js",
    "start": "node --inspect ./server.js",
    "redo": "npm run clean; npm run build; npm run start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "flow": "flow",
    "lint": "eslint --ext js,jsx src/"
  },
  "author": "Luke Persola",
  "engines": {
    "node": "7.10.0"
  },
  "dependencies": {
    "connect": "*",
    "express": "*",
    "proptypes": "^1.1.0",
    "react": "*",
    "react-redux": "*",
    "redux": "^3.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "6.x.x",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "*",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "*",
    "css-loader": "*",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.6.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.49.1",
    "html-webpack-plugin": "*",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "style-loader": "*",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.7.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!{node_modules,serve,tmp}/**",
      "!webpack.config.js"
    ],
    "coverageDirectory": "tmp/coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 0,
        "statements": 90
      }
    },
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
