{
  "name": "saliva-repl",
  "version": "0.0.1",
  "description": " Scaffold for an experimental programming interface",
  "scripts": {
    "clean": "rm -dr dist/* tmp/*",
    "clean:dist": "rm -dr dist/*",
    "build": "webpack",
    "build:noisily": "webpack --verbose",
    "start:web": "node --inspect ./web-repl/server.js",
    "start:electron": "electron electron-repl/",
    "redo": "npm run clean:dist; npm run build; npm run start:web",
    "test": "jest",
    "type": "flow --show-all-errors",
    "type-cov": "flow-coverage-report -i 'src/**/*.js{'','x'}' -x {'web-repl/server.js','src/types/**'} -o tmp/flow-coverage/",
    "lint": "eslint --ext js,jsx ."
  },
  "author": "Luke Persola",
  "engines": {
    "node": "8.9.3"
  },
  "dependencies": {
    "connect": "^3.7.0",
    "electron": "^9.0.0",
    "express": "^4.17.1",
    "json-loader": "^0.5.7",
    "mousetrap": "^1.6.5",
    "proptypes": "~1.1.0",
    "react": "~16.13.1",
    "react-dom": "^16.13.1",
    "redux": "^4.0.5",
    "yaml-loader": "^0.6.0",
    "yml-loader": "~2.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-cli": "~6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-flow": "~6.23.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "~7.0.0",
    "eslint-config-airbnb": "~18.1.0",
    "eslint-plugin-flowtype": "~5.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-react": "~7.20.0",
    "flow-bin": "^0.125.1",
    "flow-coverage-report": "^0.7.0",
    "html-webpack-plugin": "~4.3.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "^26.0.1",
    "node-sass": "^4.14.1",
    "raf": "3.4.1",
    "react-test-renderer": "~16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./test/setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!{test,node_modules,dist,tmp}/**",
      "!webpack.config.js"
    ],
    "coverageDirectory": "tmp/coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 20,
        "lines": 10,
        "statements": 10
      }
    },
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|ya?ml)$": "identity-obj-proxy"
    }
  }
}
