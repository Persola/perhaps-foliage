{
  "name": "saliva-repl",
  "version": "0.0.1",
  "description": " Scaffold for an experimental programming interface",
  "scripts": {
    "clean": "rm -dr dist/* tmp/*",
    "clean:dist": "rm -dr dist/*",
    "build": "webpack",
    "build:noisily": "webpack --verbose",
    "start:web": "node --inspect ./web-repl/server.js",
    "start:electron": "electron electron-repl/",
    "redo": "npm run clean:dist; npm run build; npm run start:web",
    "type": "flow --show-all-errors",
    "type-cov": "flow-coverage-report -i 'src/**/*.js{'','x'}' -x {'web-repl/server.js','src/types/**'} -o tmp/flow-coverage/",
    "lint": "eslint --ext js,jsx ."
  },
  "author": "Luke Persola",
  "engines": {
    "node": "14.17.1"
  },
  "dependencies": {
    "electron": "~13.1.5",
    "json-loader": "~0.5.7",
    "mousetrap": "~1.6.5",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "redux": "~4.1.0",
    "yaml-loader": "~0.6.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.14.5",
    "@babel/core": "~7.14.6",
    "@babel/eslint-parser": "~7.14.7",
    "@babel/preset-env": "~7.14.7",
    "@babel/preset-flow": "~7.14.5",
    "@babel/preset-react": "~7.14.5",
    "babel-jest": "~27.0.6",
    "babel-loader": "~8.2.2",
    "css-loader": "~5.2.6",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.6",
    "enzyme-to-json": "~3.4.4",
    "eslint": "~7.30.0",
    "eslint-config-airbnb": "~18.2.1",
    "eslint-plugin-flowtype": "~5.8.0",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-react": "~7.24.0",
    "flow-bin": "~0.154.0",
    "flow-coverage-report": "~0.8.0",
    "html-webpack-plugin": "~5.3.2",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~26.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-sass": "~6.0.1",
    "raf": "3.4.1",
    "react-test-renderer": "~16.13.1",
    "sass-loader": "~12.1.0",
    "style-loader": "~3.0.0",
    "webpack": "~5.42.0",
    "webpack-cli": "~4.7.2",
    "webpack-dev-server": "~3.11.2"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./test/setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!{test,node_modules,dist,tmp}/**",
      "!webpack.config.js"
    ],
    "coverageDirectory": "tmp/coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 20,
        "lines": 10,
        "statements": 10
      }
    },
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|ya?ml)$": "identity-obj-proxy"
    }
  }
}
