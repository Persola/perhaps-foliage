{
  "name": "saliva-repl",
  "version": "0.0.1",
  "description": " Scaffold for an experimental programming interface",
  "scripts": {
    "clean": "rm -dr dist/* tmp/*",
    "clean:dist": "rm -dr dist/*",
    "build:web": "webpack --config webpack.web.config.js",
    "build:el": "webpack --config webpack.electron.config.js",
    "build:saliva": "webpack --config webpack.saliva.config.js",
    "build:pantheon": "webpack --config webpack.pantheon.config.js",
    "start:web": "node --inspect ./web-repl/server.js",
    "start:el": "electron electron-repl/",
    "start:wds": "webpack serve --target browserslist",
    "reweb": "npm run clean:dist; npm run build:web; npm run start:web",
    "rel": "npm run clean:dist; npm run build:el; npm run start:el",
    "type": "flow --show-all-errors",
    "lint": "eslint --ext js,jsx .",
    "lint-fix": "eslint --ext js,jsx --fix ."
  },
  "author": "Luke Persola",
  "engines": {
    "node": "14.17.1"
  },
  "dependencies": {
    "core-js": "3.15.2",
    "electron": "~13.1.5",
    "immer": "^9.0.5",
    "json-loader": "~0.5.7",
    "mousetrap": "~1.6.5",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "redux": "~4.1.0",
    "redux-observable": "~2.0.0",
    "rxjs": "~7.2.0",
    "yaml-loader": "~0.6.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.14.5",
    "@babel/core": "~7.14.6",
    "@babel/eslint-parser": "~7.14.7",
    "@babel/preset-env": "~7.14.7",
    "@babel/preset-flow": "~7.14.5",
    "@babel/preset-react": "~7.14.5",
    "babel-jest": "~27.0.6",
    "babel-loader": "~8.2.2",
    "css-loader": "~5.2.6",
    "eslint": "~7.30.0",
    "eslint-config-airbnb": "~18.2.1",
    "eslint-plugin-flowtype": "~5.8.0",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-react": "~7.24.0",
    "flow-bin": "~0.154.0",
    "html-webpack-plugin": "~5.3.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-sass": "~6.0.1",
    "react-test-renderer": "~16.13.1",
    "sass-loader": "~12.1.0",
    "style-loader": "~3.0.0",
    "webpack": "~5.44.0",
    "webpack-cli": "~4.7.2",
    "webpack-dev-server": "~3.11.2"
  }
}
