{
  "name": "saliva-repl",
  "version": "0.0.1",
  "description": " Scaffold for an experimental programming interface",
  "scripts": {
    "clean": "rm -dr dist/* tmp/*",
    "clean:dist": "rm -dr dist/*",
    "build": "webpack",
    "build:noisily": "webpack --verbose",
    "start:web": "node --inspect ./web-repl/server.js",
    "start:electron": "electron electron-repl/",
    "redo": "npm run clean:dist; npm run build; npm run start:web",
    "test": "jest",
    "type": "flow",
    "type-cov": "flow-coverage-report -i 'src/**/*.js{'','x'}' -x {'web-repl/server.js','src/types/**'} -o tmp/flow-coverage/",
    "lint": "eslint --ext js,jsx ."
  },
  "author": "Luke Persola",
  "engines": {
    "node": "8.9.3"
  },
  "dependencies": {
    "connect": "~3.6.5",
    "express": "~4.16.2",
    "electron": "~1.7.9",
    "json-loader": "^0.5.7",
    "mousetrap": "~1.6.1",
    "proptypes": "~1.1.0",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "redux": "~3.7.2",
    "yaml-loader": "^0.5.0",
    "yml-loader": "~2.1.0"
  },
  "devDependencies": {
    "raf": "3.4.0",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.0.3",
    "babel-jest": "~21.2.0",
    "babel-loader": "~7.1.2",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-flow": "~6.23.0",
    "babel-preset-react": "~6.24.1",
    "css-loader": "~0.28.7",
    "enzyme": "~3.2.0",
    "enzyme-adapter-react-16": "*",
    "enzyme-to-json": "~3.3.0",
    "eslint": "~4.13.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-plugin-flowtype": "~2.40.1",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.5.1",
    "flow-bin": "^0.61.0",
    "flow-coverage-report": "^0.4.0",
    "html-webpack-plugin": "~2.30.1",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~21.2.1",
    "react-test-renderer": "~16.2.0",
    "style-loader": "~0.19.1",
    "webpack": "~3.10.0",
    "webpack-dev-server": "~2.9.7"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./test/setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!{test,node_modules,dist,tmp}/**",
      "!webpack.config.js"
    ],
    "coverageDirectory": "tmp/coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 20,
        "lines": 10,
        "statements": 10
      }
    },
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|ya?ml)$": "identity-obj-proxy"
    }
  }
}
