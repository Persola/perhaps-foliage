{
 "name": "saliva-repl",
 "displayName": "Saliva REPL",
 "version": "0.0.1",
 "description": "Larva of an experimental data structure editing interface",
 "author": "Luke Persola",
 "main": "./vscode/built/extension.js",
 "engines": {
  "node": "14.17.1",
  "vscode": "^1.58.0"
 },
 "activationEvents": [
  "onCommand:saliva-repl.helloWorld",
  "onCustomEditor:saliva-repl.saliva-repl-custom-editor"
 ],
 "contributes": {
  "commands": [
   {
    "command": "saliva-repl.helloWorld",
    "title": "Hello World"
   }
  ],
  "customEditors": [
   {
    "viewType": "saliva-repl.saliva-repl-custom-editor",
    "displayName": "Perhaps Foliage",
    "selector": [
     {
      "filenamePattern": "*.foli.json"
     }
    ],
    "priority": "default"
   }
  ]
 },
 "scripts": {
  "clean:web": "mkdir -p web/built; rm -dr web/built/*",
  "clean:el": "mkdir -p electron/built; rm -dr electron/built/*",
  "clean:vsc": "mkdir -p vscode/built; rm -dr vscode/built/*",
  "clean:saliva": "mkdir -p saliva/built; rm -dr saliva/built",
  "clean:pantheon": "mkdir -p pantheon/built; rm -dr pantheon/built",
  "build:web": "webpack --config webpack.web.config.js; cp ./web/src/server.js ./web/built/",
  "build:el": "webpack --config webpack.electron.config.js; cp ./electron/src/core-wrapper.js ./electron/built/; cp ./electron/src/renderer-preload.js ./electron/built/",
  "build:vsc": "webpack --config webpack.vscode.config.js",
  "build:saliva": "webpack --config webpack.saliva.config.js",
  "build:pantheon": "webpack --config webpack.pantheon.config.js",
  "start:web": "node --inspect ./web/built/server.js",
  "start:el": "electron electron/built/core-wrapper.js",
  "start:vsc": "code --extensionDevelopmentPath=.",
  "reweb": "npm run clean:web; npm run build:web; npm run start:web",
  "rel": "npm run clean:el; npm run build:el; npm run start:el",
  "type": "tsc --noEmit",
  "lint": "eslint --ext ts,tsx .",
  "lint-fix": "eslint --ext ts,tsx --fix ."
 },
 "dependencies": {
  "core-js": "3.15.2",
  "electron": "~13.1.5",
  "immer": "^9.0.5",
  "json-loader": "~0.5.7",
  "mousetrap": "~1.6.5",
  "prettier": "^2.2.1",
  "react": "~17.0.2",
  "react-dom": "~17.0.2",
  "redux": "~4.1.0",
  "redux-observable": "~2.0.0",
  "rxjs": "~7.2.0",
  "source-map-loader": "^3.0.0",
  "ts-loader": "^9.2.3",
  "yaml-loader": "~0.6.0"
 },
 "devDependencies": {
  "@electron/typescript-definitions": "^8.9.5",
  "@types/node": "14.17.1",
  "@types/react": "^17.0.14",
  "@types/redux": "^3.6.0",
  "@types/vscode": "^1.58.0",
  "@typescript-eslint/eslint-plugin": "^4.28.4",
  "@typescript-eslint/parser": "^4.28.4",
  "css-loader": "~5.2.6",
  "eslint": "~7.30.0",
  "eslint-config-airbnb": "~18.2.1",
  "eslint-plugin-import": "~2.23.4",
  "eslint-plugin-jsx-a11y": "~6.4.1",
  "eslint-plugin-react": "~7.24.0",
  "html-webpack-plugin": "~5.3.2",
  "node-polyfill-webpack-plugin": "^1.1.4",
  "node-sass": "~6.0.1",
  "react-test-renderer": "~16.13.1",
  "sass-loader": "~12.1.0",
  "style-loader": "~3.0.0",
  "typescript": "^4.3.5",
  "webpack": "~5.44.0",
  "webpack-cli": "~4.7.2",
  "webpack-dev-server": "~3.11.2"
 }
}
