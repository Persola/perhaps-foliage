{
 "name": "saliva-repl",
 "version": "0.0.1",
 "description": "Larva of an experimental data structure editing interface",
 "scripts": {
  "clean:web": "mkdir -p web/built; rm -dr web/built/*",
  "clean:el": "mkdir -p electron/built; rm -dr electron/built/*",
  "clean:saliva": "mkdir -p src/extension-staging-area/saliva/built; rm -dr src/xtension-staging-area/saliva/built",
  "clean:pantheon": "mkdir -p src/extension-staging-area/pantheon/built; rm -dr src/extension-staging-area/pantheon/built",
  "build:web": "webpack --config webpack.web.config.js; cp ./web/src/server.js ./web/built/",
  "build:el": "webpack --config webpack.electron.config.js; cp ./electron/src/core-wrapper.js ./electron/built/; cp ./electron/src/renderer-preload.js ./electron/built/",
  "build:saliva": "webpack --config webpack.saliva.config.js",
  "build:pantheon": "webpack --config webpack.pantheon.config.js",
  "start:web": "node --inspect ./web/built/server.js",
  "start:el": "electron electron/built/core-wrapper.js",
  "reweb": "npm run clean:web; npm run build:web; npm run start:web",
  "rel": "npm run clean:el; npm run build:el; npm run start:el",
  "type": "tsc --noEmit",
  "lint": "eslint --ext ts,tsx .",
  "lint-fix": "eslint --ext ts,tsx --fix ."
 },
 "author": "Luke Persola",
 "engines": {
  "node": "14.17.1"
 },
 "dependencies": {
  "core-js": "3.15.2",
  "electron": "~13.1.5",
  "immer": "^9.0.5",
  "json-loader": "~0.5.7",
  "mousetrap": "~1.6.5",
  "prettier": "^2.2.1",
  "react": "~17.0.2",
  "react-dom": "~17.0.2",
  "redux": "~4.1.0",
  "redux-observable": "~2.0.0",
  "rxjs": "~7.2.0",
  "source-map-loader": "^3.0.0",
  "ts-loader": "^9.2.3",
  "yaml-loader": "~0.6.0"
 },
 "devDependencies": {
  "@babel/cli": "~7.14.5",
  "@babel/core": "~7.14.6",
  "@babel/eslint-parser": "~7.14.7",
  "@babel/plugin-proposal-class-properties": "^7.14.5",
  "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
  "@babel/preset-env": "~7.14.7",
  "@babel/preset-react": "~7.14.5",
  "@babel/preset-typescript": "^7.14.5",
  "@types/react": "^17.0.14",
  "@types/redux": "^3.6.0",
  "@typescript-eslint/eslint-plugin": "^4.28.4",
  "@typescript-eslint/parser": "^4.28.4",
  "babel-jest": "~27.0.6",
  "babel-loader": "~8.2.2",
  "css-loader": "~5.2.6",
  "eslint": "~7.30.0",
  "eslint-config-airbnb": "~18.2.1",
  "eslint-plugin-import": "~2.23.4",
  "eslint-plugin-jsx-a11y": "~6.4.1",
  "eslint-plugin-react": "~7.24.0",
  "html-webpack-plugin": "~5.3.2",
  "node-polyfill-webpack-plugin": "^1.1.4",
  "node-sass": "~6.0.1",
  "react-test-renderer": "~16.13.1",
  "sass-loader": "~12.1.0",
  "style-loader": "~3.0.0",
  "typescript": "^4.3.5",
  "webpack": "~5.44.0",
  "webpack-cli": "~4.7.2",
  "webpack-dev-server": "~3.11.2"
 }
}
