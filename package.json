{
  "name": "saliva-repl",
  "version": "0.0.1",
  "description": " Scaffold for an experimental programming interface",
  "scripts": {
    "clean": "rm -dr dist/* tmp/*",
    "clean:dist": "rm -dr dist/*",
    "build": "webpack",
    "build:noisily": "webpack --verbose",
    "start:web": "node --inspect ./web-repl/server.js",
    "start:electron": "electron electron-repl/",
    "redo": "npm run clean:dist; npm run build; npm run start:web",
    "test": "jest",
    "type": "flow --show-all-errors",
    "type-cov": "flow-coverage-report -i 'src/**/*.js{'','x'}' -x {'web-repl/server.js','src/types/**'} -o tmp/flow-coverage/",
    "lint": "eslint --ext js,jsx ."
  },
  "author": "Luke Persola",
  "engines": {
    "node": "8.9.3"
  },
  "dependencies": {
    "connect": "^3.6.6",
    "electron": "^3.0.12",
    "express": "^4.16.4",
    "json-loader": "^0.5.7",
    "mousetrap": "^1.6.2",
    "proptypes": "~1.1.0",
    "react": "~16.6.3",
    "react-dom": "^16.6.3",
    "redux": "~4.0.1",
    "yaml-loader": "^0.5.0",
    "yml-loader": "~2.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "~6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "~10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-flow": "~6.23.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "~4.13.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-plugin-flowtype": "~2.40.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.5.1",
    "flow-bin": "^0.89.0",
    "flow-coverage-report": "^0.6.1",
    "html-webpack-plugin": "~3.2.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "^24.8.0",
    "node-sass": "^4.11.0",
    "raf": "3.4.0",
    "react-test-renderer": "~16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "~0.19.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.1"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./test/setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!{test,node_modules,dist,tmp}/**",
      "!webpack.config.js"
    ],
    "coverageDirectory": "tmp/coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 20,
        "lines": 10,
        "statements": 10
      }
    },
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|ya?ml)$": "identity-obj-proxy"
    }
  }
}
