{
  "name": "saliva-repl",
  "version": "0.0.1",
  "description": " sdlfwhoflfawelfea",
  "main": "serve.js",
  "scripts": {
    "clean": "rm -dr serve/* tmp/*",
    "clean:serve": "rm -dr serve/*",
    "build": "webpack",
    "build:noisily": "webpack --verbose",
    "webpack-dev-server": "webpack-dev-server server.js",
    "start": "node --inspect ./server.js",
    "redo": "npm run clean:serve; npm run build; npm run start",
    "test": "jest",
    "type": "flow",
    "type-cov": "flow-coverage-report -i 'src/**/*.js{'','x'}' -x {'server.js','src/types/**'} -o tmp/flow-coverage/",
    "lint": "eslint --ext js,jsx ."
  },
  "author": "Luke Persola",
  "engines": {
    "node": "7.10.0"
  },
  "dependencies": {
    "connect": "~3.6.3",
    "express": "~4.15.4",
    "json-loader": "^0.5.7",
    "mousetrap": "^1.6.1",
    "proptypes": "^1.1.0",
    "react": "~15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "~5.0.5",
    "redux": "^3.7.1",
    "yaml-loader": "^0.5.0",
    "yml-loader": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "~6.1.2",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "~6.24.1",
    "css-loader": "~0.28.7",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.6.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.49.1",
    "flow-coverage-report": "^0.3.0",
    "html-webpack-plugin": "~2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "style-loader": "~0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.7.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!{node_modules,serve,tmp}/**",
      "!webpack.config.js"
    ],
    "coverageDirectory": "tmp/coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 20,
        "lines": 10,
        "statements": 10
      }
    },
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
