{
  "name": "saliva-repl-web",
  "displayName": "Saliva REPL Web",
  "version": "0.0.2",
  "description": "Web build of Saliva REPL",
  "author": "Luke Persola",
  "scripts": {
    "clean": "mkdir -p ./built; rm -dr ./built/*",
    "build": "npm run clean; webpack --config ./webpack.config.js; cp ./src/server.js ./built/",
    "start": "node ./built/server.js",
    "rdt": "node ./start-rdt-server.js",
    "redo": "npm run clean; npm run build; npm run start",
    "type": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx ."
  },
  "dependencies": {
    "saliva-repl": "file:../shared"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "css-loader": "~5.2.6",
    "eslint": "~7.30.0",
    "eslint-config-airbnb": "~18.2.1",
    "html-webpack-plugin": "~5.3.2",
    "json-loader": "~0.5.7",
    "redux-devtools-cli": "^1.0.0-4",
    "source-map-loader": "^3.0.0",
    "style-loader": "~3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "~5.44.0",
    "webpack-cli": "~4.7.2",
    "yaml-loader": "~0.6.0"
  }
}
