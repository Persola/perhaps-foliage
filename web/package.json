{
  "name": "saliva-repl-web",
  "displayName": "Saliva REPL Web",
  "version": "0.0.2",
  "description": "Web build of Saliva REPL",
  "author": "Luke Persola",
  "scripts": {
    "clean": "mkdir -p ./built; rm -dr ./built/*",
    "build": "npm run clean; webpack --config ./webpack.config.js; cp ./src/server.js ./built/",
    "start": "node ./built/server.js",
    "rdt": "node ./start-rdt-server.js",
    "redo": "npm run clean; npm run build; npm run start",
    "type": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx ."
  },
  "dependencies": {
    "saliva-repl": "file:../shared"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~5.30.7",
    "@typescript-eslint/parser": "~5.30.7",
    "css-loader": "~6.7.1",
    "eslint": "~8.20.0",
    "eslint-config-airbnb": "~19.0.4",
    "html-webpack-plugin": "~5.5.0",
    "redux-devtools-cli": "1.0.0-4",
    "source-map-loader": "~4.0.0",
    "style-loader": "~3.3.1",
    "ts-loader": "~9.3.1",
    "typescript": "~4.7.4",
    "webpack": "~5.73.0",
    "webpack-cli": "~4.10.0"
  }
}
