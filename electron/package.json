{
  "name": "saliva-repl-electron",
  "displayName": "Saliva REPL Electron",
  "version": "0.0.2",
  "description": "electron build of Saliva REPL",
  "author": "Luke Persola",
  "engines": {
    "node": "14.17.1"
  },
  "scripts": {
    "clean": "mkdir -p built; rm -dr built/*",
    "build": "npm run clean; webpack --config ./webpack.config.js; cp ./src/core-wrapper.js ./built/; cp ./src/renderer-preload.js ./built/",
    "start": "electron ./built/core-wrapper.js",
    "rdt": "node ./start-rdt-server.js",
    "redo": "npm run clean; npm run build; npm run start",
    "type": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx ."
  },
  "dependencies": {
    "electron": "~13.1.5",
    "saliva-repl": "file:../shared"
  },
  "devDependencies": {
    "@electron/typescript-definitions": "^8.9.6",
    "@types/node": "~18.6.0",
    "@typescript-eslint/eslint-plugin": "~5.30.7",
    "@typescript-eslint/parser": "~5.30.7",
    "css-loader": "~6.7.1",
    "eslint": "~8.20.0",
    "eslint-config-airbnb": "~19.0.4",
    "html-webpack-plugin": "~5.5.0",
    "json-loader": "~0.5.7",
    "node-polyfill-webpack-plugin": "~2.0.0",
    "redux-devtools-cli": "1.0.0-4",
    "source-map-loader": "~4.0.0",
    "style-loader": "~3.3.1",
    "ts-loader": "~9.3.1",
    "typescript": "~4.7.4",
    "webpack": "~5.73.0",
    "webpack-cli": "~4.10.0",
    "yaml-loader": "~0.6.0"
  }
}
