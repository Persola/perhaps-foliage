{
  "name": "saliva-repl-electron",
  "displayName": "Saliva REPL Electron",
  "version": "0.0.2",
  "description": "electron build of Saliva REPL",
  "author": "Luke Persola",
  "engines": {
    "node": "14.17.1"
  },
  "scripts": {
    "clean": "mkdir -p built; rm -dr built/*",
    "build": "npm run clean; webpack --config ./webpack.config.js; cp ./src/core-wrapper.js ./built/; cp ./src/renderer-preload.js ./built/",
    "start": "electron ./built/core-wrapper.js",
    "rdt": "node ./start-rdt-server.js",
    "redo": "npm run clean; npm run build; npm run start",
    "type": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx ."
  },
  "dependencies": {
    "electron": "~13.1.5",
    "saliva-repl": "file:../shared"
  },
  "devDependencies": {
    "@electron/typescript-definitions": "^8.9.5",
    "@types/node": "14.17.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "css-loader": "~5.2.6",
    "eslint": "~7.30.0",
    "eslint-config-airbnb": "~18.2.1",
    "html-webpack-plugin": "~5.3.2",
    "json-loader": "~0.5.7",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "redux-devtools-cli": "^1.0.0-4",
    "source-map-loader": "^3.0.0",
    "style-loader": "~3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "~5.44.0",
    "webpack-cli": "~4.7.2",
    "yaml-loader": "~0.6.0"
  }
}
