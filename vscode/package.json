{
  "name": "saliva-repl-vscode",
  "displayName": "Saliva REPL VSCode",
  "version": "0.0.4",
  "description": "VSCode base extension for the Perhaps Foliage Editor",
  "author": "Luke Persola",
  "publisher": "persola",
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.58.0"
  },
  "scripts": {
    "clean": "mkdir -p ./dist; rm -dr ./dist/*",
    "build": "npm run clean; tsc",
    "type": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx ."
  },
  "dependencies": {
    "saliva-repl": "file:../shared"
  },
  "devDependencies": {
    "@types/vscode": "~1.69.0",
    "@typescript-eslint/eslint-plugin": "~5.30.7",
    "@typescript-eslint/parser": "~5.30.7",
    "css-loader": "~6.7.1",
    "eslint": "~8.20.0",
    "eslint-config-airbnb": "~19.0.4",
    "html-webpack-plugin": "~5.5.0",
    "source-map-loader": "~4.0.0",
    "style-loader": "~3.3.1",
    "ts-loader": "~9.3.1",
    "typescript": "~4.7.4",
    "webpack": "~5.73.0",
    "webpack-cli": "~4.10.0"
  }
}
